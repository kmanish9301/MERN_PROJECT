{
  "version": 2,
  "builds": [
    {
      "src": "app.js",  
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app.js" 
    },
    {
      "src": "/v1/(.*)",
      "dest": "/app.js"  
    },
    {
      "src": "/uploads/(.*)",  
      "dest": "/uploads/$1"
    },
    {
      "src": "/userImages/(.*)",
      "dest": "/userImages/$1"
    }
  ]
}
